{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "src": ["./src"],
      "@app": [
        "./src/app"
      ],
      "@processes": [
        "./src/processes"
      ],
      "@screens": [
        "./src/screens"
      ],
      "@shared": [
        "./src/shared"
      ]
    }
  },
  "include": [
    "src"
  ]
}
